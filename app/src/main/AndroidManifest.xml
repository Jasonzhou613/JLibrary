<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ttsea.jlibrary.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.ttsea.jlibrary.sample.ComponentActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.PhotoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.AsyncHttpActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.MyTimerActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.RoundImageActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.SmoothCheckBoxActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.ToggleButtonActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.PageViewActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ttsea.jlibrary.sample.NetWorkConnectionActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>